{
    "name": "@lwc/reactive-service",
    "version": "0.36.0",
    "description": "LWC Reactive Service",
    "license": "MIT",
    "main": "dist/commonjs/es2017/reactive.js",
    "module": "dist/modules/es2017/reactive.js",
    "typings": "lib/reactive.d.ts",
    "scripts": {
        "clean": "rm -rf dist/ lib/",
        "test": "jest",
        "build": "tsc && rollup --config ./scripts/rollup/rollup.config.js",
        "start": "concurrently \"yarn run build:playground --watch\" \"yarn run serve:playground\"",
        "build:playground": "rollup -c playground/rollup.config.js",
        "serve:playground": "node playground/server.js"
    },
    "devDependencies": {
        "express": "^4.15.2"
    },
    "lwc": {
        "modules": {
            "reactive-service": "dist/modules/es2017/reactive.js"
        }
    },
    "publishConfig": {
        "access": "public"
    }
}
